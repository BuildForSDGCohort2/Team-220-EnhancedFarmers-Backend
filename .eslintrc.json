{
  "env": {
    "es6": true,
    "node": true
  },
  "parser": "babel-eslint",
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": 2015,
    "sourceType": "module"
  },
  "rules": {
    "no-console": 0,
    "no-empty": 0,
    "no-undef": 0,
    "global-require": 0,
    "comma-dangle": 0,
    "guard-for-in": 0,
    "no-restricted-syntax": 0,
    "consistent-return": 0,
    "object-curly-newline": 0,
    "no-irregular-whitespace": 0,
    "quotes": ["error", "double"],
    "semi": ["error", "always"]
  }
}
